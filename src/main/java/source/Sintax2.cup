    public int buscarSimbolo (String id, ArrayList<ArrayList<String>> lista) {
        int index = -1;
        for (int i = 0; i < lista.size(); i++) {
            if (lista.get(i).get(1).equals(id)) {
                index = i;
                break;
            }
        }
        return index;
    }

